root=true

[*]
indent_style = space
indent_size = 2
continuation_indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8

[*.java]
indent_size = 4
continuation_indent_size = 8

[*.xml]
indent_size = 4
continuation_indent_size = 8

[*.gradle]
indent_size = 4
continuation_indent_size = 8
